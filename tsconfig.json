{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "lib": ["esnext"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "types": ["vite/client", "@histoire/plugin-vue/components", "element-plus/global"]
  },
  "include": ["src/**/*", "src/**/*.vue"]
}
